<?xml version="1.0" encoding="UTF-8"?>
<web-app version="4.0" xmlns="http://xmlns.jcp.org/xml/ns/javaee" xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" xsi:schemaLocation="http://xmlns.jcp.org/xml/ns/javaee http://xmlns.jcp.org/xml/ns/javaee/web-app_4_0.xsd">
    <servlet>
        <servlet-name>addTicketServlet</servlet-name>
        <servlet-class>gihan.addTicketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>java</servlet-name>
        <servlet-class>addTicketsServlet.java</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>addTicketsServlet</servlet-name>
        <servlet-class>gihan.addTicketsServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>updateTicketServlet</servlet-name>
        <servlet-class>gihan.updateTicketServlet</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_ticket</servlet-name>
        <servlet-class>gihan.delete_ticket</servlet-class>
    </servlet>
    <servlet>
        <servlet-name>delete_ticketServlet</servlet-name>
        <servlet-class>gihan.delete_ticketServlet</servlet-class>
    </servlet>
    <servlet-mapping>
        <servlet-name>addTicketServlet</servlet-name>
        <url-pattern>/addTicketServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>java</servlet-name>
        <url-pattern>/java</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>addTicketsServlet</servlet-name>
        <url-pattern>/addTicketsServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>updateTicketServlet</servlet-name>
        <url-pattern>/updateTicketServlet</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_ticket</servlet-name>
        <url-pattern>/delete_ticket</url-pattern>
    </servlet-mapping>
    <servlet-mapping>
        <servlet-name>delete_ticketServlet</servlet-name>
        <url-pattern>/delete_ticketServlet</url-pattern>
    </servlet-mapping>
    <session-config>
        <session-timeout>
            30
        </session-timeout>
    </session-config>
</web-app>
